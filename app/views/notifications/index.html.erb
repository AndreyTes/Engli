<h1> Notifications: <%= current_user.username %> </h1>
<div class = "container-fluid">
  <div class = "row">
    <div class = "col-md-9.col-md-offset-1 ">
      <% @notifications.each do |notification| %>
        <%= render 'notifications/notification', notification: notification %>
      <% end %>
    </div>
  </div>
</div>

