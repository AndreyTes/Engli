<tr>
  <td><%= example.example %></td>
  <td><%= example.user.username %></td>
  <td><%= date_formatter(example.created_at) %></td>
  <td>
    <%= link_to phrase_example_vote_path(example.phrase, example , vote: 'up'), method: :post  do%>
      <div><span class= "glyphicon glyphicon-chevron-up upvote" aria-hidden="true""></span>
      </div>  
    <% end %>
    <%= example.get_likes.size - example.get_dislikes.size %>
    <%= link_to phrase_example_vote_path(example.phrase, example, vote: 'down'), method: :post  do%>
      <div><span class= "glyphicon glyphicon-chevron-down downvote" aria-hidden="true""></span>
      </div>  
    <% end %>
  </td>
  <td>
    <% if example.author?(current_user) || example.phrase.author?(current_user)%>
    <%= link_to 'Delete', phrase_example_path(example.phrase, example) , method: :delete %>
    <% end %>
  </td>
</tr>
